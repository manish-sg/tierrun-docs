rewrites() {
  return {
      beforeFiles: [
          // if the host is `docs.tier.run`,
          // this rewrite will be applied
          {
              source: '/:path*',
              has: [
                  {
                      type: 'host',
                      value: 'docs.acme.com',
                  },
              ],
              destination: '/docs/:path*',
          },
      ]
  }
}